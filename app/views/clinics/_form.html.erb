<div class="container">
  <div class="col-xs-6 col-xs-offset-3">
      <%= form_for @clinic do |f| %>
      <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name, class: 'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :price %>
        <%= f.number_field :price, class: 'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :date_time %>
        <%= f.datetime_local_field :date_time, class: 'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :location_name %>
        <%= f.text_field :location_name, class: 'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :location_street_one %>
        <%= f.text_field :location_street_one, class: 'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :location_street_two %>
        <%= f.text_field :location_street_two, class: 'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :city %>
        <%= f.text_field :city, class: 'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :state %>
        <%= f.text_field :state, class: 'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :zipcode %>
        <%= f.number_field :zipcode, class: 'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :total_spots %>
        <%= f.number_field :total_spots, class: 'form-control' %>
      </div>

        <%= f.submit :class => 'btn btn-default' %>
      <% end %>
  </div>
</div>


